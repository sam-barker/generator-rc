{
  "name": "generator-rjs",
  "version": "1.0.2",
  "description": "Yeoman generator for react",
  "scripts": {
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-barker/generator-rjs.git"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "files": [
    "generators/**/*"
  ],
  "author": "Samuel Barker <spbarker1994@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/sam-barker/generator-rjs#readme",
  "dependencies": {
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "semistandard": "^13.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "semistandard": {
    "ignore": [
      "**/node_modules/"
    ],
    "globals": [
      "describe",
      "it",
      "expect"
    ]
  }
}
